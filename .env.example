# HackApp Configuration Template
# Copy this file to .env and fill in your values

# Middleware Configuration
MIDDLEWARE_HOST=localhost
MIDDLEWARE_PORT=5000
MIDDLEWARE_TOKEN=hackathon_demo_token_change_in_production

# Mock Service Configuration
MOCK_SERVICE_URL=http://localhost:5001
MOCK_SERVICE_ENABLED=true

# External Service API Keys (if using real services)
# OPENAI_API_KEY=your_key_here
# AZURE_SPEECH_KEY=your_key_here

# Security
SECRET_KEY=change_this_to_random_string_in_production

# Logging
LOG_LEVEL=INFO
AUDIT_LOG_PATH=./logs/audit.log

# Agent Configuration
AGENT_AUTO_INSERT=true
AGENT_INSERT_DELAY_MS=10

# DXCare Simulation
DXCARE_WINDOW_TITLE=DXCare
